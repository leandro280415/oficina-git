package br.edu.fmf.modelo;

public class Gato {

	private String cor;
	private String nome;
	private int idade;
	
	
	// metodos//
	public void setCor(String cor ) {
		this.cor = cor ;
	}
	
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public void setIdade(int idade) {
		this.idade = idade;
	}
	
}
